{
  "workbench.editor.enablePreview": false,
  "[typescript]" : {
      "editor.formatOnSave": true,
  },
  "window.zoomLevel": 1,
  "explorer.confirmDelete": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#161616",
  "vim.statusBarColors.replace": "#5f0000",
  "vim.statusBarColors.insert": "#6b630b",
  "vim.visualModeKeyBindingsNonRecursive": [
      {
          "before": [
              "p"
          ],
          "after": [
              "p",
              "g",
              "v",
              "y"
          ]
      }
  ],
  "vim.useSystemClipboard": true,
  "vim.leader": "|",
  "vim.hlsearch": true,
  "vim.easymotionMarkerFontFamily": "Menlo",
  "vim.easymotion": false,
  "vim.easymotionMarkerYOffset": 0,
  "vim.easymotionMarkerHeight": 16,
  "vim.easymotionMarkerFontSize": "16",
  "editor.lineNumbers": "relative",
  "workbench.colorCustomizations": {
      "statusBar.background": "#005f5f",
      "statusBar.noFolderBackground": "#005f5f",
      "statusBar.debuggingBackground": "#005f5f"
  },
  "workbench.iconTheme": "vscode-icons",
  "breadcrumbs.enabled": true
}